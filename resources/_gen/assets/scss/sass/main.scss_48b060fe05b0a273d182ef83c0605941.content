body{font-size:16px;font-family:arimo,sans-serif;background:#f0f8fd;margin:0;margin-bottom:1em}header .wrapper{background:#005e95;padding-top:1em;padding-bottom:1em;padding-right:10vw;color:#fff;display:flex;align-items:center}header .wrapper .site-tittle{font-size:2em}nav{box-shadow:0 3px 6px 0 rgba(0,0,0,.16);position:relative}nav .wrapper{display:flex;justify-content:center;padding-top:1em}nav .wrapper ul li{display:inline-block;color:#005e95;font-weight:700;font-size:1.5em;padding-right:1.5em;padding-left:1.5em}section{padding-right:10vw;padding-top:3.5em;color:#005e95;background-repeat:no-repeat;min-height:40vh;background-size:contain}section h2{font-size:4em;margin:0}section .medium{font-size:1.5em;margin:0;padding:0}section .big{font-size:2em;margin:0;padding:0}section.money .text{width:fit-content;margin-top:5vh}.sheet-gird{display:flex;margin-left:2em;margin-top:5vh}.sheet-gird .sheet-block{margin-left:3em;padding:1em 1em 1em 1.5em;width:22em;box-shadow:0 3px 6px 0 rgba(0,0,0,.16);background-color:#fff}.sheet-gird .sheet-block .title{font-size:2em;color:#005e95;margin:0;padding-bottom:1em}.sheet-gird .sheet-block .sheet-image{max-width:100%}
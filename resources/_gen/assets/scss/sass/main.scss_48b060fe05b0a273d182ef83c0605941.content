body{font-size:16px;font-family:arimo,sans-serif;background:#f0f8fd;margin:0}header .wrapper{background:#005e95;padding-top:1em;padding-bottom:1em;padding-right:10vw;color:#fff;display:flex;align-items:center}header .wrapper .site-tittle{font-size:2em}header .wrapper .site-tittle a{color:#fff;text-decoration:none;font-weight:700}nav{box-shadow:0 3px 6px 0 rgba(0,0,0,.16);position:relative}nav .wrapper{display:flex;justify-content:center}nav .wrapper ul{margin-block-start:0;margin-block-end:0;margin-inline-start:0;margin-inline-end:0;padding-inline-start:0;padding-top:.75em;padding-bottom:.75em;display:grid;grid-template-columns:auto auto auto auto;grid-gap:50px}nav .wrapper ul li{color:#005e95;font-weight:700;font-size:1.5em;list-style-type:none}nav .wrapper ul li a{text-decoration:none;color:#005e95}nav .wrapper ul li a:visited{color:#005e95}nav .wrapper ul li a:hover{text-decoration:underline}section{padding-right:10vw;color:#005e95;background-repeat:no-repeat;min-height:40vh;background-size:contain;margin-bottom:5vh;padding-bottom:2em}section h2{font-size:4em;margin:0;font-weight:400}section .medium{font-size:1.5em;margin:0;padding:0}section .big{font-size:2em;margin:0;padding:0}section.money{padding-top:3.5em}section.money .text{width:fit-content;margin-top:5vh}section.about-us .quarter-text{font-size:1.25em;width:20vw}section.about-us .contact-us{padding-top:2em}section.about-us .contact-us button{width:15em;height:2.5em;border:none;border-radius:6px;box-shadow:0 3px 6px 0 rgba(0,0,0,.16);background-color:#007ac1;font-size:20px;font-weight:700;font-stretch:normal;font-style:normal;line-height:1.15;letter-spacing:normal;text-align:center;color:#fefeff}section.about-us .contact-us button a{color:#fefeff}.grid-display{display:grid;padding-top:1.5em;grid-template-columns:auto auto auto}.grid-display div{padding:1em 1em 1em 1.5em;box-shadow:0 3px 6px 0 rgba(0,0,0,.16);background-color:#fff;margin:1.5em;margin-right:0}.grid-display .title{font-size:2em;color:#005e95;margin:0;padding-bottom:1em}.grid-display .title a{color:#005e95;text-decoration:none}.grid-display .title a:hover{text-decoration:underline}.grid-display img{max-width:100%}.grid-display img.bordered{border:solid 1px #005e95}.grid-display .sub-title{padding-top:.25em;font-size:1.25em;color:#005e95;margin:0;padding-bottom:1em}footer{background-color:#add9ff;font-size:1.25em;height:4.5em;display:flex;align-items:center;justify-content:center;color:#fff}footer.bottom{position:fixed;width:100vw;bottom:0}footer ul{display:grid;grid-template-columns:auto auto auto auto;grid-gap:50px;margin-block-start:0;margin-block-end:0;margin-inline-start:0;margin-inline-end:0;padding-inline-start:0}footer ul li{list-style-type:none}footer ul li a{color:#fff;text-decoration:none}footer ul li a:visited{color:#fff}footer ul li a:hover{text-decoration:underline}.content{padding-right:10vw;padding-left:10vw}.content h1{padding-bottom:0;margin-bottom:0}.content{padding-bottom:10vh}.breadcrumb{margin:0;padding:1.5em;padding-inline-start:0}.breadcrumb li{list-style-type:none}.breadcrumb li a{color:#000;font-weight:700}@media(max-width:1610px){section .grid-display{grid-template-columns:auto auto}}@media(max-width:1030px){header .wrapper{padding-right:0}body{font-size:18px}header .wrapper{justify-content:center;font-size:1.5em}section .big{font-size:1.75em}section .grid-display{grid-template-columns:auto}section.about-us .quarter-text{font-size:1.25em;width:75vw}}@media(max-width:768px){body{font-size:14px}footer ul{grid-gap:1.5em}}@media(max-width:500px){nav .wrapper ul{display:grid;grid-template-columns:auto auto auto auto;grid-gap:.5em}section h2{font-size:2.5em}section.money span{display:block;direction:ltr;text-align:right}section.about-us h2{font-size:3em}body{font-size:12px}}
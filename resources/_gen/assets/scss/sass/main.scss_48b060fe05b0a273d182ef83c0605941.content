body{font-size:16px;font-family:arimo,sans-serif;background:#f0f8fd;margin:0}header .wrapper{background:#005e95;padding-top:1em;padding-bottom:1em;padding-right:10vw;color:#fff;display:flex;align-items:center}header .wrapper .site-tittle{font-size:2em}header .wrapper .site-tittle a{color:#fff;text-decoration:none;font-weight:700}nav{box-shadow:0 3px 6px 0 rgba(0,0,0,.16);position:relative}nav .wrapper{display:flex;justify-content:center;padding-top:1em}nav .wrapper ul li{display:inline-block;color:#005e95;font-weight:700;font-size:1.5em;padding-right:1.5em;padding-left:1.5em}nav .wrapper ul li a{text-decoration:none;color:#005e95}nav .wrapper ul li a:visited{color:#005e95}nav .wrapper ul li a:hover{text-decoration:underline}section{padding-right:10vw;color:#005e95;background-repeat:no-repeat;min-height:40vh;background-size:contain;margin-bottom:5vh;padding-bottom:2em}section h2{font-size:4em;margin:0;font-weight:400}section .medium{font-size:1.5em;margin:0;padding:0}section .big{font-size:2em;margin:0;padding:0}section.money{padding-top:3.5em}section.money .text{width:fit-content;margin-top:5vh}section.about-us .quarter-text{font-size:1.25em;width:20vw}section.about-us .contact-us{padding-top:2em}section.about-us .contact-us button{width:349px;height:45px;border:none;border-radius:6px;box-shadow:0 3px 6px 0 rgba(0,0,0,.16);background-color:#007ac1;font-size:20px;font-weight:700;font-stretch:normal;font-style:normal;line-height:1.15;letter-spacing:normal;text-align:center;color:#fefeff}.grid-display{display:grid;padding-top:1.5em;grid-template-columns:auto auto auto}.grid-display div{padding:1em 1em 1em 1.5em;box-shadow:0 3px 6px 0 rgba(0,0,0,.16);background-color:#fff;margin:1.5em;margin-right:0}.grid-display .title{font-size:2em;color:#005e95;margin:0;padding-bottom:1em}.grid-display .title a{color:#005e95;text-decoration:none}.grid-display .title a:hover{text-decoration:underline}.grid-display img{max-width:100%}.grid-display img.bordered{border:solid 1px #005e95}.grid-display .sub-title{padding-top:.25em;font-size:1.25em;color:#005e95;margin:0;padding-bottom:1em}footer{background-color:#add9ff;font-size:1.25em;height:4.5em;display:flex;align-items:center;justify-content:center;color:#fff}